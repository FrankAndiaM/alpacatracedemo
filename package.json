  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "npm run lint --fix",
    "prett": "prettier --write src/**/*.{ts,tsx}",
    "husky": "./node_modules/.bin/husky install"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleDirectories": [
      ".",
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "tsx",
      "ts"
    ],
    "moduleNameMapper": {
      "^~config(.*)$": "<rootDir>/src/config$1",
      "^~hooks(.*)$": "<rootDir>/src/hooks$1",
      "^~models(.*)$": "<rootDir>/src/models$1",
      "^~redux-store(.*)$": "<rootDir>/src/redux-store$1",
      "^~routes(.*)$": "<rootDir>/src/routes$1",
      "^~services(.*)$": "<rootDir>/src/services$1",
      "^~utils(.*)$": "<rootDir>/src/utils$1",
      "^~ui(.*)$": "<rootDir>/src/ui$1",
      "^~assets(.*)$": "<rootDir>/src/ui/assets$1",
      "^~atoms(.*)$": "<rootDir>/src/ui/atoms$1",
      "^~molecules(.*)$": "<rootDir>/src/ui/molecules$1",
      "^~organisms(.*)$": "<rootDir>/src/ui/organisms$1",
      "^~pages(.*)$": "<rootDir>/src/ui/pages$1",
      "^~templates(.*)$": "<rootDir>/src/ui/templates$1"
    },
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
